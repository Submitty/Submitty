{
  "name": "submitty/submitty",
  "config": {
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require": {
    "browscap/browscap-php": "7.6.0",
    "cboden/ratchet": "0.4.4",
    "doctrine/dbal": "3.8.3",
    "doctrine/orm": "3.6.2",
    "egulias/email-validator": "4.0.4",
    "lcobucci/jwt": "5.3.0",
    "league/commonmark": "2.8.0",
    "maennchen/zipstream-php": "3.1.1",
    "onelogin/php-saml": "4.3.1",
    "php-ds/php-ds": "1.7.0",
    "ramsey/uuid": "4.9.2",
    "symfony/cache": "6.4.7",
    "symfony/config": "6.4.4",
    "symfony/http-foundation": "6.4.14",
    "symfony/routing": "6.1.11",
    "textalk/websocket": "1.6.3",
    "twig/markdown-extra": "^3.23",
    "twig/twig": "3.23.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "1.2.0",
    "php-mock/php-mock-phpunit": "2.15.0",
    "phpstan/phpstan": "2.1.40",
    "phpstan/phpstan-deprecation-rules": "2.0.3",
    "phpstan/phpstan-doctrine": "2.0.17",
    "phpstan/phpstan-strict-rules": "2.0.8",
    "phpunit/phpunit": "10.5.62",
    "submitty/php-codesniffer": "3.0.1",
    "symfony/finder": "^6.4",
    "symfony/twig-bundle": "^6.4"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "php vendor/bin/phpcs --extensions=php ./app",
    "static-analysis": "php vendor/bin/phpstan analyze"
  }
}
