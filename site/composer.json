{
  "name": "submitty/submitty",
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require": {
    "aptoma/twig-markdown": "3.4.1",
    "browscap/browscap-php": "7.2.0",
    "cboden/ratchet": "0.4.4",
    "doctrine/annotations": "1.14.3",
    "doctrine/dbal": "3.6.6",
    "doctrine/orm": "2.16.2",
    "egulias/email-validator": "3.2.6",
    "lcobucci/jwt": "5.0.0",
    "league/commonmark": "1.6.7",
    "maennchen/zipstream-php": "3.1.0",
    "onelogin/php-saml": "4.1.0",
    "php-ds/php-ds": "1.4.1",
    "ramsey/uuid": "4.7.4",
    "symfony/cache": "6.3.2",
    "symfony/config": "6.3.2",
    "symfony/http-foundation": "6.3.2",
    "symfony/polyfill-php80": "1.27.0",
    "symfony/routing": "6.1.11",
    "textalk/websocket": "1.6.3",
    "twig/twig": "3.7.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
    "php-mock/php-mock-phpunit": "2.7.2",
    "phpstan/phpstan": "1.10.28",
    "phpstan/phpstan-deprecation-rules": "1.1.4",
    "phpstan/phpstan-doctrine": "1.3.42",
    "phpunit/phpunit": "9.6.9",
    "submitty/php-codesniffer": "2.5.0"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs",
    "static-analysis": "phpstan analyze app public/index.php socket/index.php"
  }
}
