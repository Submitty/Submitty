{
    "autograding": {
        "compilation_to_runner": [
            "**/*.out",
            "**/*.class"
        ],
        "compilation_to_validation": [
            "test*/STDOUT*.txt",
            "test*/STDERR*.txt"
        ],
        "submission_to_compilation": [
            "**/*.cpp",
            "**/*.cxx",
            "**/*.c",
            "**/*.h",
            "**/*.hpp",
            "**/*.hxx",
            "**/*.java"
        ],
        "submission_to_runner": [
            "**/*.py",
            "**/*.pdf"
        ],
        "submission_to_validation": [
            "**/README.txt",
            "textbox_*.txt",
            "**/*.pdf"
        ],
        "use_checkout_subdirectory": "",
        "work_to_details": [
            "test*/*.txt",
            "test*/*_diff.json",
            "**/README.txt",
            "textbox_*.txt",
            "test*/textbox_*.txt",
            "test01/STDOUT.txt",
            "test01/STDERR.txt"
        ]
    },
    "autograding_method": "jailed_sandbox",
    "container_options": {
        "container_image": "ubuntu:custom",
        "single_port_per_container": false,
        "use_router": false
    },
    "testcases": [
        {
            "containers": [
                {
                    "commands": [
                        "submitty_count -l python depth loop *.py"
                    ],
                    "container_image": "ubuntu:custom",
                    "container_name": "container0",
                    "outgoing_connections": [],
                    "server": false
                }
            ],
            "dispatcher_actions": [],
            "points": 10,
            "pre_commands": [],
            "single_port_per_container": false,
            "title": "Python - Determine Loop Depth",
            "use_router": false,
            "validation": [
                {
                    "actual_file": "STDOUT.txt",
                    "comparison": "le",
                    "deduction": 1.0,
                    "description": "Loop Depth",
                    "failure_message": "Must have less than four nested loops",
                    "method": "intComparison",
                    "show_actual": "always",
                    "show_message": "always",
                    "term": 3
                },
                {
                    "actual_file": "STDERR.txt",
                    "deduction": 0.0,
                    "description": "Standard Error (STDERR.txt)",
                    "method": "warnIfNotEmpty",
                    "show_actual": "on_failure",
                    "show_message": "on_failure"
                }
            ]
        },
        {
            "max_submissions": 20,
            "penalty": -0.1,
            "points": -5,
            "title": "Submission Limit",
            "type": "FileCheck"
        }
    ]
}
