{
  "name": "submitty/submitty",
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require": {
    "aptoma/twig-markdown": "3.4.1",
    "cboden/ratchet": "0.4.4",
    "doctrine/annotations": "1.14.3",
    "doctrine/dbal": "2.13.9",
    "doctrine/orm": "2.12.3",
    "egulias/email-validator": "3.2.5",
    "lcobucci/jwt": "4.3.0",
    "league/commonmark": "1.6.7",
    "maennchen/zipstream-php": "2.2.6",
    "onelogin/php-saml": "4.1.0",
    "php-ds/php-ds": "1.4.1",
    "ramsey/uuid": "4.2.3",
    "symfony/cache": "5.4.23",
    "symfony/config": "5.4.21",
    "symfony/http-foundation": "5.4.24",
    "symfony/polyfill-php80": "1.27.0",
    "symfony/routing": "5.4.22",
    "textalk/websocket": "1.6.3",
    "twig/twig": "3.6.1"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
    "php-mock/php-mock-phpunit": "2.7.2",
    "phpstan/phpstan": "1.10.25",
    "phpstan/phpstan-deprecation-rules": "1.1.3",
    "phpunit/phpunit": "10.2.4",
    "submitty/php-codesniffer": "2.5.0"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs",
    "static-analysis": "phpstan analyze app public/index.php socket/index.php"
  }
}
