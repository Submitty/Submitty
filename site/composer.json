{
  "name": "submitty/submitty",
  "config": {
    "platform": {
      "php": "7.2"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require": {
    "aptoma/twig-markdown": "3.4.1",
    "cboden/ratchet": "0.4.4",
    "doctrine/annotations": "1.13.2",
    "doctrine/orm": "2.12.2",
    "egulias/email-validator": "3.1.2",
    "lcobucci/jwt": "3.4.6",
    "league/commonmark": "1.6.7",
    "maennchen/zipstream-php": "2.1.0",
    "php-ds/php-ds": "1.4.1",
    "ramsey/uuid": "4.2.3",
    "spatie/commonmark-highlighter": "2.1.1",
    "symfony/cache": "4.4.42",
    "symfony/config": "4.4.41",
    "symfony/http-foundation": "4.4.42",
    "symfony/polyfill-php80": "1.25.0",
    "symfony/routing": "4.4.41",
    "textalk/websocket": "1.5.8",
    "twig/twig": "2.15.1"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
    "php-mock/php-mock-phpunit": "2.6.0",
    "phpstan/phpstan": "1.7.2",
    "phpstan/phpstan-deprecation-rules": "1.0",
    "phpunit/phpunit": "8.5.26",
    "submitty/php-codesniffer": "2.5.0"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs",
    "static-analysis": "phpstan analyze app public/index.php socket/index.php"
  }
}
