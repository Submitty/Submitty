{% if viewer.getQueueAnnouncementMessage() | length > 0 %}
    <div id="announcement" data-testid="announcement">
        <h2>Office Hours Queue Announcements:</h2>
        {% include 'Vue.twig' with {
            type: "component",
            name: "Markdown",
            args: {
                content: viewer.getQueueAnnouncementMessage()
            }
        } only %}
    </div>
{% endif %}
